import java.util.*;

public class SumOfDigitsOfnum {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();

        System.out.println(sumOfDigit(n));
    }

    public static int sumOfDigit(int n){

        int temp=n;
        int sum=0;
        while(temp!=0){
        int rem=temp%10;
        sum+=rem;
        temp=temp/10;
        }

        return sum;

    } 
}
